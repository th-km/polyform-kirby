{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": 0,
    "func-names": 0,
    "no-param-reassign": 1,
    "operator-assignment": 1,
    "prefer-arrow-callback": 0,
    "arrow-parens": 0,
    "no-else-return": 0,
    "comma-dangle": 0,
    "prefer-const": 0,
    "prettier/prettier": ["error"]
  }
}
